{
	"name": "@debrix/compiler",
	"version": "0.1.0-alpha.1",
	"description": "JavaScript distribution of debrix's compiler.",
	"keywords": [
		"debrix",
		"compiler",
		"node",
		"wasm"
	],
	"homepage": "https://github.com/debrixjs/compiler/tree/main/packages/compiler#readme",
	"repository": "https://github.com/debrixjs/compiler",
	"license": "MIT",
	"author": "Elias Skogevall",
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js",
			"types": "./index.d.ts"
		},
		"./node": {
			"import": "./wasm/index.mjs",
			"require": "./wasm/index.js",
			"types": "./wasm/index.d.ts"
		},
		"./wasm": {
			"import": "./wasm/index.mjs",
			"require": "./wasm/index.js",
			"types": "./wasm/index.d.ts"
		}
	},
	"files": [
		"./dist/",
		"./types/",
		"./install.mjs"
	],
	"scripts": {
		"build": "node build.mjs",
		"build:release": "node build.mjs --production",
		"test": "node test/index.mjs",
		"_postinstall": "node install.mjs",
		"prepublishOnly": "npm run build:release"
	},
	"devDependencies": {
		"@rollup/pluginutils": "^4.2.1",
		"cargo-cp-artifact": "0.1.x",
		"concurrently": "^7.4.0",
		"esbuild": "^0.15.10",
		"esbuild-plugin-alias": "^0.2.1",
		"follow-redirects": "^1.15.2",
		"rollup": "2.x",
		"rollup-plugin-base64": "1.x",
		"rollup-plugin-terser": "7.x"
	},
	"peerDependencies": {
		"@debrix/internal": "*"
	},
	"engines": {
		"node": ">=10"
	}
}
